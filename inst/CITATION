citHeader("To cite 'deseasonalize' in publications use:")

citEntry(entry = "Article",
  title        = "Optimal Deseasonalization for Monthly and Daily Geophysical Time Series",
  author       = personList(
  as.person("A. I. McLeod"),
  as.person("Hyukjun Gweon")
  ),
  journal      = "Journal of Environmental Statistics",
  year         = "2013",
  volume       = "4",
  number       = "11",
  url          = "http://www.jenvstat.org/v04/i11",
  textVersion  =
  paste("A. I. McLeod and Hyukjun Gweon (2012).",
        "Optimal Deseasonalization for Monthly and Daily Geophysical Time Series",
        "Journal of Environmental Statistics 4(11).",
        "URL http://www.jenvstat.org/v04/i11")
)


